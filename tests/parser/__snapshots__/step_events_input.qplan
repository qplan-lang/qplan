step id="prepare" desc="데이터 준비" {
  var 0 -> total
  var [1,2,3] -> nums
}

step id="sum" desc="합산" {
  each n in nums {
    math add a=total b=n -> total
  }
}

step id="branch" desc="분기" {
  if total >= 3 {
    jump to="final"
  }
  print "branch continued"
}

step id="final" desc="마무리" {
  print total
  print sum
  return summary=sum
}
