{
  "name": "qplan-language",
  "displayName": "QPlan Language",
  "description": "Syntax highlighting for QPlan scripts",
  "publisher": "local",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "qplan",
        "aliases": ["QPlan", "qplan"],
        "extensions": [".qplan"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "qplan",
        "scopeName": "source.qplan",
        "path": "./syntaxes/qplan.tmLanguage.json"
      }
    ]
  }
}
