{
  "name": "qplan-language",
  "displayName": "QPlan Language",
  "description": "Syntax highlighting for QPlan scripts",
  "publisher": "local",
  "repository": {
    "type": "git",
    "url": "https://github.com/qplan-lang/qplan.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configurationDefaults": {
      "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "keyword.control.flow.qplan",
            "settings": {
              "foreground": "#E9323C"
            }
          },
          {
            "scope": "support.function.qplan",
            "settings": {
              "foreground": "#599ed6"
            }
          },
          {
            "scope": "variable.other.readwrite.qplan",
            "settings": {
              "foreground": "#d8bd82"
            }
          },
          {
            "scope": "entity.other.attribute-name.qplan",
            "settings": {
              "foreground": "#D4D4D4"
            }
          },
          {
            "scope": "keyword.operator.assignment.qplan",
            "settings": {
              "foreground": "#808080"
            }
          }
        ]
      }
    },
    "languages": [
      {
        "id": "qplan",
        "aliases": [
          "QPlan",
          "qplan"
        ],
        "extensions": [
          ".qplan"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "qplan",
        "scopeName": "source.qplan",
        "path": "./syntaxes/qplan.tmLanguage.json"
      }
    ]
  }
}